{
	"name": "obsidian-instapaper",
	"version": "0.5.0",
	"description": "This is the official Instapaper plugin for Obsidian.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"lint": "eslint src",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"instapaper",
		"obsidian"
	],
	"author": "Instapaper",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"dotenv": "^16.3.1",
		"esbuild": "0.19.11",
		"obsidian": "latest",
		"tslib": "2.6.2",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"oauth-1.0a": "^2.2.6"
	}
}